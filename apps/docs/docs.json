{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Codemod.com",
  "colors": {
    "primary": "#7faa09",
    "light": "#d5ff61",
    "dark": "#0B151E"
  },
  "background": {
    "color": {
      "dark": "#0b141e",
      "light": "#FFF"
    }
  },
  "favicon": "/favicon.svg",
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Getting Started",
        "icon": "book",
        "description": "Get started with Codemod",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart"]
          },
          {
            "group": "Codemod Platform",
            "pages": ["migrations", "insights", "codemod-studio"]
          },
          {
            "group": "OSS & Community",
            "pages": [
              "cli/cli-reference",
              {
                "group": "Packages",
                "pages": [
                  "cli/packages/quickstart",
                  "cli/packages/package-structure",
                  "cli/packages/building-workflows"
                ]
              },
              "/model-context-protocol"
            ]
          },
          {
            "group": "Quick Links",
            "pages": ["external-links/codemod-registry"]
          },
          {
            "group": "Other Resources",
            "pages": ["more-resources/changelog"]
          }
        ]
      },
      {
        "dropdown": "Guides",
        "icon": "chalkboard",
        "description": "Learn more with hands-on guides.",
        "groups": [
          {
            "group": "Guides & Examples",
            "pages": ["guides/introduction"]
          },
          {
            "group": "Migrations",
            "pages": [
              "guides/migrations/react-18-19",
              "guides/migrations/nuxt-3-4",
              "guides/migrations/zod-3-4",
              "guides/migrations/msw-v1-v2",
              "guides/migrations/pnpm-catalog",
              "guides/external-links/radix-reka",
              "guides/external-links/fastify-v3-v4",
              "guides/external-links/meteor-v2-v3",
              "guides/external-links/valibot"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/codemod-logo-light.svg",
    "dark": "/logo/codemod-logo-dark.svg",
    "href": "https://codemod.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Community",
        "href": "https://go.codemod.com/community"
      }
    ],
    "primary": {
      "type": "github",
      "href": "https://github.com/codemod/codemod"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/codemod",
      "github": "https://github.com/codemod-com",
      "linkedin": "https://www.linkedin.com/company/codemod"
    }
  },
  "redirects": [
    {
      "source": "/more-resources/legal/telemetry-compliance",
      "destination": "https://codemod.com/telemetry-compliance"
    },
    {
      "source": "/more-resources/legal/privacy-policy",
      "destination": "https://codemod.com/privacy-policy"
    },
    {
      "source": "/more-resources/legal/terms-and-conditions",
      "destination": "https://codemod.com/terms-and-conditions"
    },
    {
      "source": "/platform/plan",
      "destination": "/migrations"
    },
    {
      "source": "/platform/automate",
      "destination": "/codemod-studio"
    },
    {
      "source": "/platform/orchestrate",
      "destination": "/migrations"
    },
    {
      "source": "/platform/track",
      "destination": "/insights"
    },
    {
      "source": "/cli/workflows/quickstart",
      "destination": "/cli/packages/quickstart"
    },
    {
      "source": "/cli/workflows/package-structure",
      "destination": "/cli/packages/package-structure"
    },
    {
      "source": "/cli/workflows/building-workflows",
      "destination": "/cli/packages/building-workflows"
    },
    {
      "source": "/cli/workflows",
      "destination": "/cli/packages/quickstart"
    },
    {
      "source": "/cli/mcp",
      "destination": "/model-context-protocol"
    },
    {
      "source": "/deploying-codemods/cli",
      "destination": "/cli/cli-reference"
    },
    {
      "source": "/building-codemods/package-requirements",
      "destination": "/cli/packages/package-structure"
    },
    {
      "source": "/building-codemods/codemod2",
      "destination": "/cli/packages/quickstart"
    },
    {
      "source": "/scanner",
      "destination": "/insights"
    }
  ],
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  }
}
