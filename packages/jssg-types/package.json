{
  "name": "@codemod.com/jssg-types",
  "version": "1.3.2",
  "private": false,
  "description": "Comprehensive types package for ast-grep integrations and codemod development",
  "keywords": [
    "ast",
    "ast-grep",
    "codemod",
    "types"
  ],
  "author": "Codemod, Inc.",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.d.ts"
    },
    "./langs/*": {
      "types": "./src/langs/*.d.ts",
      "default": "./src/langs/*.d.ts"
    },
    "./main": {
      "types": "./src/main.d.ts",
      "default": "./src/main.d.ts"
    },
    "./workflow": {
      "types": "./src/workflow.d.ts",
      "default": "./src/workflow.d.ts"
    }
  },
  "files": [
    "./src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@codemod.com/tsconfig": "workspace:*",
    "typescript": "5.5.4"
  }
}
